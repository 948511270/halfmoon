/* packageMarking/pages/marking/marking.wxss */
.container{
  margin-top: 100rpx;
}
.my-application{
  width: 94%;
  margin-bottom: 50rpx;
  border-radius: 15rpx;
  background-color: #fff;
}
.my-application-info{
  padding: 15rpx 0 15rpx 3rpx;
  border-radius: 15rpx 15rpx 0rpx 0rpx ;
  background-color: #fff;
}
.my-application-score{
  border-radius: 0rpx 0rpx 15rpx 15rpx ;
  background-color: #84bffb;
  padding: 10rpx 10rpx 10rpx 20rpx;
  font-size: larger;
  color: #fff;
}